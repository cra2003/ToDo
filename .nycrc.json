{
  "all": true,
  "check-coverage": false,
  "lines": 90,
  "functions": 90,
  "branches": 90,
  "statements": 90,
  "include": ["src/**/*.js"],
  "exclude": [
    "tests/**",
    "**/*.test.js",
    "**/*.spec.js",
    "node_modules/**",
    "coverage/**",
    "*.db",
    "*.log"
  ],
  "reporter": ["text", "text-summary", "html"],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output"
}
